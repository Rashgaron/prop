<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="background" stylesheets="@Css/general.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Vistas.Controllers.DatosOponenteCtrl">
   <children>
      <Label alignment="CENTER" layoutX="1.0" layoutY="45.0" prefHeight="35.0" prefWidth="600.0" text="Opponent's email">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <Label alignment="CENTER" layoutX="1.0" layoutY="87.0" prefHeight="17.0" prefWidth="600.0" text="Write the email of your opponent. It has to be a registered user" />
      <TextField fx:id="lblEmail" layoutX="226.0" layoutY="127.0" styleClass="input" />
      <Button fx:id="btnPlay" layoutX="278.0" layoutY="208.0" mnemonicParsing="false" onAction="#goToPlay" prefHeight="25.0" prefWidth="41.0" text="Play">
         <styleClass>
            <String fx:value="btn-green" />
            <String fx:value="btn" />
         </styleClass></Button>
      <Button fx:id="btnGoBack" layoutX="280.0" layoutY="254.0" mnemonicParsing="false" onAction="#goBack" text="Back">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btnGoBack" />
         </styleClass></Button>
      <Button fx:id="closeButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Button" visible="false" />
   </children>
</AnchorPane>
